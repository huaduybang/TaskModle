@*@{
        ViewBag.Title = "Index";
        Layout = "~/Views/Shared/_Layout.cshtml";
    }*@

@model List<Task_BaoCaoPhanTich.User>
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link href="~/Assets/Boostrap/css/bootstrap.css" rel="stylesheet" />
    <link href="~/Assets/css/Index.css" rel="stylesheet" />
</head>

<body>
    <div class="container">
        <table class="table table-bordered">
            <thead class="box color-table">
                <tr>
                    <th class="box1">Đại lý</th>
                    <th class="box2">Thời gian</th>
                    <th class="pull-center">
                        <button type="button" class="btn btn-success">
                            <div class="text-color">@Html.ActionLink("Thêm", "Create", "Users")</div>
                        </button>
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@Html.DisplayFor(modelItem => item.TenDL)</td>
                        <td>@DateTime.Now</td>
                        <td>
                            <div class="btn-group pull-right" role="group" aria-label="Basic example">
                                <div class="btn btn-link">@Html.ActionLink("Sửa", "Edit", new { id = item.ID })</div>
                                <div class="btn btn-link">@Html.ActionLink("Xóa", "Delete", new { id = item.ID })</div>
                                <div class="btn btn-link">@Html.ActionLink("Xem kết quả", "Details", new { id = item.ID })</div>
                            </div>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
        <div class="panel-footer">
            <button type="button" class="btn-success btn-lg">
                <div>@*@Html.ActionLink("Trở lại","Index")*@</div>
            </button>
        </div>
    </div>
    <script src="/Boostrap/js/bootstrap.js"></script>
</body>
</html>